{
  "name": "starter-kit",
  "version": "1.0.0",
  "description": "React, Redux, Router v4",
  "main": "index.js",
  "author": "Michael Arnaldi",
  "license": "MIT",
  "scripts": {
    "dev": "better-npm-run dev",
    "compile": "better-npm-run compile",
    "serve": "better-npm-run serve"
  },
  "dependencies": {
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.8",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-runtime": "^6.18.0",
    "compression": "^1.6.2",
    "connect-history-api-fallback": "^1.3.0",
    "express": "^4.14.0",
    "history": "^4.4.0",
    "ip": "^1.1.4",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-motion": "^0.4.5",
    "react-onclickoutside": "^5.7.1",
    "react-redux": "^4.4.6",
    "react-router": "next",
    "redbox-react": "^1.3.3",
    "redux": "^3.6.0",
    "redux-observable": "^0.12.2",
    "rxjs": "^5.0.0-rc.4",
    "styled-components": "^1.1.1"
  },
  "devDependencies": {
    "babel-plugin-transform-remove-console": "^6.8.0",
    "babel-preset-react-optimize": "^1.0.1",
    "better-npm-run": "^0.0.13",
    "cssnano": "^3.8.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.24.1",
    "nodemon": "^1.11.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "webpack": "^1.13.3",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.2"
  },
  "betterScripts": {
    "dev": {
      "command": "node server/main.js",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "compile": {
      "command": "webpack",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "serve": {
      "command": "node server/prod.js",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    }
  }
}
